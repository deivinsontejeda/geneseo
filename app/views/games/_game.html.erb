<div id="game_panel">
<table class="game round">
  <tr>
  <td> <center> <%= render 'games/title' %> </td> </br>
  </tr>
  <tr>
  </tr>
  <% unless @game.board.nil? %>
    <tr> <td> <center> <%= render 'games/turn_display' %> </td> </br> </tr>
    <div id="board_display"> <tr> <td> <center> <%= render 'games/board' %> </td>  </br> </tr> </div>
  <% end %>
  <td> <strong> Game: <%= @game.id %> <%= button_to "Quit!", { controller: "games", action: :destroy, user_id: @current_user }, remote: true, method: :delete %> </td>
</table>
</div>
